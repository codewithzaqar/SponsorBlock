{
    "name": "YTSponsorSkip",
    "version": "1.0",
    "description": "Skip youtube video sponsors.",
    "content_scripts": [
      {
        "matches": [
          "https://*.youtube.com/*"
        ],
        "js": [
          "contentScript.js"
        ]
      }
    ],
    "permissions": [
      "tabs",
      "storage"
    ],
    "browser_action": {
      "default_title": "SponsorBlock",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts":["background.js"]
    },
    "manifest_version": 2
  }