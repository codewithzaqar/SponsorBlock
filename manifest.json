{
    "name": "YTSponsorSkip",
    "version": "1.0",
    "description": "Skip youtube video sponsors",
    "content_scripts": [
      {
        "matches": [
          "https://*.youtube.com/*"
        ],
        "js": [
          "ContentScript.js"
        ]
      }
    ],
    "manifest_version": 2
  }